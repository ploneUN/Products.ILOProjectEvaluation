<?xml version="1.0"?>

<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="iloprojectevaluation_images"
         meta_type="Filesystem Directory View"
         directory="ILOProjectEvaluation/skins/iloprojectevaluation_images"/>
 <object name="iloprojectevaluation_styles"
         meta_type="Filesystem Directory View"
         directory="ILOProjectEvaluation/skins/iloprojectevaluation_styles"/>
 <object name="iloprojectevaluation_templates"
         meta_type="Filesystem Directory View"
         directory="ILOProjectEvaluation/skins/iloprojectevaluation_templates"/>
 
 <skin-path name="*">
  <layer insert-after="custom" 
         name="iloprojectevaluation_images"/>
  <layer insert-after="custom" 
         name="iloprojectevaluation_styles"/>
  <layer insert-after="custom" 
         name="iloprojectevaluation_templates"/>
 </skin-path>

</object>